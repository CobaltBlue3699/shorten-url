{
  "name": "@shorten-url/source",
  "version": "0.0.1-0",
  "license": "MIT",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "version": "npm run format",
    "preversion": "npm run lint",
    "postversion": "git push && git push --tags",
    "build": "npx nx build frontend && nx build backend && echo 'build completed!'",
    "test": "npx nx test frontend && nx test backend && echo 'testing complete!'",
    "e2e:f": "nx e2e frontend-e2e",
    "e2e:b": "nx e2e backend-e2e",
    "start:f": "npx nx build frontend --watch",
    "start:b": "set NODE_TLS_REJECT_UNAUTHORIZED=0&& npx nx serve backend --watch --host=0.0.0.0",
    "lint": "nx lint frontend --fix && nx lint frontend-e2e --fix && nx lint backend --fix && nx lint backend-e2e && nx lint keycloak-connect && nx lint base --fix && echo 'lint completed!'",
    "format": "prettier --write \"apps/frontend/src/**/*.ts\" \"apps/frontend-e2e/src/**/*.ts\" \"apps/backend/src/**/*.ts\" \"apps/backend-e2e/src/**/*.ts\" \"libs/backend/**/src/**/*.ts\" \"libs/frontend/**/src/**/*.ts\"",
    "release:prerelease": "npm version prerelease && echo 'waiting for ci commands'",
    "release:prepatch": "npm version prepatch && echo 'waiting for ci commands'",
    "release:patch": "npm version patch && echo 'waiting for ci commands'",
    "release:preminor": "npm version preminor && echo 'waiting for ci commands'",
    "release:minor": "npm version minor && echo 'waiting for ci commands'",
    "release:premajor": "npm version premajor && echo 'waiting for ci commands'",
    "release:major": "npm version major && echo 'waiting for ci commands'"
  },
